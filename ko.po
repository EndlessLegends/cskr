#. "CHIRP_RANDOM:42"
msgid "최면 능력을 가진 두꺼비가 현재 애완용으로 최고의 인기를 얻고 있습니다. 불면증 해소에 딱이예요! #최면두꺼비"
msgstr ""
#. "CHIRP_RANDOM:43"
msgid "공중 부양하는 고양이가 인터넷의 새로운 대세로 떠올랐습니다. #너넨못하냥"
msgstr ""
#. "CHIRP_RANDOM:44"
msgid "@OCP 회사의 새 제품인 자동 청소로봇은 매우 훌륭합니다! 요리, 세탁, 청소 이 모든 것을 완벽하게 수행하네요! #로봇도우미"
msgstr ""
#. "CHIRP_RANDOM:45"
msgid "@british_lord : 여행할 생각에 매우 즐겁습니다. 민간 우주선이 지구를 떠나 달을 여행할 수 있게 되었어요. #우주관광중"
msgstr ""
#. "CHIRP_RANDOM:46"
msgid "@과학자 : 와우! 일식 현상이 이번 여름에 북극에서 관측될거라는군. 보고싶다! #자연의신비"
msgstr ""
#. "CHIRP_RANDOM:47"
msgid "@캣워크 : 은색이 바로 새로운 검정색이었으니, 이제는 주황색이 새로운 검정색이 될 때가 됐어. #주황색이검정색이니라"
msgstr ""
#. "CHIRP_RANDOM:48"
msgid "@loshermanosumbrellas : 비를 맞으면 완벽하게 녹아 분해되는 우산이 상점에서 판매됩니다! #해킹"
msgstr ""
#. "CHIRP_RANDOM:49"
msgid "@반짝반짝_눈_이식 : 다양한 색상과 패턴으로 당신의 눈을 뱀보다 더 반짝이게 만들어 줄 수 있습니다. 지금 신청하세요! #아름다워라"
msgstr ""
#. "CHIRP_RANDOM:50"
msgid "@veridian_dynamics : 벽에 몇번 부딪치더라도 저에겐 이보다 기쁠 수 없어요! 통제된 환경에서 로켓 테스트를 실시했습니다. #로켓연구가"
msgstr ""
#. "CHIRP_RANDOM:51"
msgid "@라자러스_프로젝트 : 대기업들이 사업에 실패한 사람들의 재기를 도와주는 회생 프로젝트에 대한 투자를 중단해버렸네. 그거 하나 믿고 버텼는데... 난 이제 망했어. #어쩌냐"
msgstr ""
