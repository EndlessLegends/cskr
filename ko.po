#. "CHIRP_POLICY[StudentHealthcare]:9"
msgid "#의료보험은 원래 #기본적으로 무상으로 제공해야 하는 것 아닌가요? #궁금 #문제제기"
msgstr ""
#. "CHIRP_POLICY[StudentHealthcare]:10"
msgid "이럴 때 #학생 #의료보험이 제공된다니 정말 다행이에요. #아이고 #아파 #몸살"
msgstr ""
#. "CHIRP_POLICY[StudentHealthcare]:11"
msgid "전 건강하니까 #무상 #의료 서비스는 필요 없어요. #자신만만 #건강한삶"
msgstr ""
#. "CHIRP_POLICY[FreeLunch]:0"
msgid "내 #점심 #쿠폰 본 사람? 아무래도 잃어버린 것 같아."
